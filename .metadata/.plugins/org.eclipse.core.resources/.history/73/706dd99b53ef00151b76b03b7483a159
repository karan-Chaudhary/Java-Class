
public class Toyato {

	public static void main(String[] args) {
       Tractor trac=new Tractor();
       Car c=new Car();
       System.out.println("Car");
       c.cc(100);
       c.color("black");
       c.door(4);
       c.milage(250);
       System.out.println("Tractor");
       trac.cc(230);
       trac.color("red");
       trac.door(4);
       trac.milage(200);
	}

}
class Car implements Vehicles{

	@Override
	public void color(String color) {
		System.out.println(color);
		
		
	}

	@Override
	public void cc(int cc) {
		System.out.println(cc);	
	}

	@Override
	public void door(int door) {
		System.out.println(door);
		
	}

	@Override
	public void milage(int speed) {
		System.out.println(speed+"kmpl");
		
	}
	
	
}

class Tractor implements Vehicles{

		@Override
		public void color(String color) {
			System.out.println(color);
			
		}

		@Override
		public void cc(int cc) {
			System.out.println(cc);
			
		}

		@Override
		public void door(int door) {
			System.out.println(door);
			
		}

		@Override
		public void milage(int speed) {
			System.out.println(speed+"kmpl");
			
		}
	
}
